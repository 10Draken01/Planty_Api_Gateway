{
  "name": "api-plants",
  "version": "1.0.0",
  "description": "Microservicio de gestión de plantas - Catálogo de plantas disponibles",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "dev:watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' -r tsconfig-paths/register src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "seed": "node scripts/seed-database.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "plants",
    "catalog",
    "typescript",
    "microservice",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
